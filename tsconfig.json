{
  "extends": "./.nuxt/tsconfig.json",
  "include": ["types", "nuxt.config.ts", "**/*.ts", "**/*.vue"],
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "baseUrl": ".",
    "paths": {
      "#app": ["./node_modules/nuxt/dist/app"],
      "~/*": ["*"]
    },
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  }
}
